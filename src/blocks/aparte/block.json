{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "capitainewp/aparte",
	"version": "1.0",
	"title": "Aparté",
	"category": "capitainewp",
	"icon": { "foreground": "#83bd71", "src": "flag" },
	"description": "Affiche un contenu en aparté dans un bloc bleu.",
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": ".wp-block-capitainewp-aparte__title",
			"default": "Aparté: "
		},
		"content": {
			"type": "array",
			"source": "children",
			"selector": ".wp-block-capitainewp-aparte__content"
		}
	},
	"textdomain": "capitainewp-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
